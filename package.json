{
  "name": "@template/cli",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A basic Effect CLI application",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "bun build ./src/bin.ts --outdir=./dist --target=bun",
    "clean": "rm -rf dist/*",
    "check": "biome check src && biome format src && bun test --typecheck",
    "lint": "biome check src",
    "lint-fix": "biome check --write src",
    "format": "biome format --write src",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "changeset-version": "changeset version",
    "changeset-publish": "bun run build && bun test && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/cli": "^0.66.10",
    "@effect/language-service": "^0.1.0",
    "@effect/platform": "latest",
    "@effect/platform-bun": "latest",
    "@types/node": "^22.5.2",
    "bun-types": "^1.2.18",
    "effect": "latest"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
    }
  },
  "dependencies": {
    "@effect/schema": "^0.75.5"
  }
}
